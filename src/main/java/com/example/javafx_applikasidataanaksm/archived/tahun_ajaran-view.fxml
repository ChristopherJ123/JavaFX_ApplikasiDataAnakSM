<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="240.0" prefWidth="430.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.javafx_applikasidataanaksm.archived.controllers.TahunAjaranController">
   <children>
      <TableView fx:id="table" layoutX="20.0" layoutY="20.0" prefHeight="200.0" prefWidth="200.0">
        <columns>
          <TableColumn fx:id="table_id" prefWidth="75.0" text="id" />
          <TableColumn fx:id="table_tahun" prefWidth="124.0" text="tahun" />
        </columns>
      </TableView>
      <Button fx:id="tambahTahunAjaranBtn" layoutX="260.0" layoutY="20.0" mnemonicParsing="false" onAction="#handleTambahTahunAjaranBtn" prefWidth="150.0" text="Tambah Tahun Ajaran" />
      <Button fx:id="editTahunAjaranBtn" layoutX="260.0" layoutY="70.0" mnemonicParsing="false" prefWidth="150.0" text="Edit Tahun Ajaran" />
      <Button fx:id="deleteTahunAjaranBtn" layoutX="260.0" layoutY="120.0" mnemonicParsing="false" prefWidth="150.0" text="Delete Tahun Ajaran" />
      <Separator layoutX="240.0" layoutY="20.0" orientation="VERTICAL" prefHeight="200.0" prefWidth="3.0" />
   </children>
</AnchorPane>
