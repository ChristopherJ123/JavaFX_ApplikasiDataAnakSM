<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml" fx:controller="com.example.javafx_applikasidataanaksm.controllers.users.RegisterController" stylesheets="@../../assets/css/styles.css">
    <VBox alignment="CENTER" spacing="10.0">
        <TextField fx:id="usernameField" promptText="Username" styleClass="text-field"/>
        <PasswordField fx:id="passwordField" promptText="Password" styleClass="password-field"/>
    </VBox>
    <VBox spacing="10.0" alignment="CENTER">
        <Button text="Register" onAction="#handleRegister" styleClass="button button_add"/>
        <Button text="Back to Login" onAction="#handleBackToLogin" styleClass="button button_back"/>
    </VBox>
</VBox>
